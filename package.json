{
  "name": "vue-fixed-header",
  "version": "2.0.2",
  "description": "Simple and cross-browser friendly fixed header component for Vue.js.",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue-plugin",
    "vue-component",
    "typescript"
  ],
  "repository": {
    "url": "potato4d/vue-fixed-header",
    "type": "git"
  },
  "main": "dist/vue-fixed-header.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "yarn serve",
    "serve": "parcel ./example/index.html -d docs",
    "build": "yarn clean;bili ./src/index.ts --format cjs --js typescript2",
    "clean": "rm -rf {.rpt2_cache,dist}",
    "format": "prettier './{src,example}/**/*.{js,ts,vue}' --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "gh-pages": "gh-pages -d docs -m '[ci skip] deploy'"
  },
  "author": {
    "name": "potato4d",
    "email": "mail@potato4d.me"
  },
  "bili": {
    "filename": "vue-fixed-header.js"
  },
  "devDependencies": {
    "@types/jest": "23.3.14",
    "@types/node": "10.12.27",
    "@vue/component-compiler-utils": "2.6.0",
    "@vue/test-utils": "1.0.0-beta.25",
    "all-contributors-cli": "5.11.0",
    "bili": "1.6.5",
    "bulma": "0.6.2",
    "codecov": "3.1.0",
    "gh-pages": "2.0.1",
    "jest": "23.6.0",
    "jsdom": "12.2.0",
    "jsdom-global": "3.0.2",
    "parcel-bundler": "1.10.2",
    "prettier": "1.14.3",
    "rollup-plugin-typescript2": "0.17.1",
    "standard-version": "4.4.0",
    "ts-jest": "23.10.4",
    "typescript": "3.1.2",
    "vue-template-compiler": "2.5.17"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-class-component": "^6.2.0",
    "vue-hot-reload-api": "^2.3.1",
    "vue-property-decorator": "^7.2.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
